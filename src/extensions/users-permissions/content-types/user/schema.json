{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "user_customized_recipe": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-customized-recipe.user-customized-recipe",
      "mappedBy": "user"
    },
    "favorite_recipe": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::favorite-recipe.favorite-recipe",
      "mappedBy": "user"
    },
    "meal_plans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::meal-plan.meal-plan",
      "mappedBy": "users_permissions_user"
    },
    "user_meal_plans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-meal-plan.user-meal-plan",
      "mappedBy": "user"
    },
    "elimination_calendars": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::elimination-calendar.elimination-calendar",
      "mappedBy": "user"
    },
    "diet_preference": {
      "type": "enumeration",
      "enum": [
        "Carnivore",
        "Ketovore"
      ],
      "required": true
    },
    "user_interests": {
      "type": "component",
      "repeatable": true,
      "component": "user.user-interests"
    },
    "bmi": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bmi.bmi",
      "mappedBy": "user"
    }
  }
}
